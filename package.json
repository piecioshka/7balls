        "babel-preset-stage-0": "^6.5.0",
        "debug": "^2.2.0",
        "expose-loader": "^0.7.1",
        "imports-loader": "^0.6.5",
        "json-loader": "^0.5.4",
        "lodash.assign": "^4.2.0",
        "lodash.isobject": "^3.0.2",
        "phaser": "^2.6.1",
        "pixi": "^0.3.1",
        "script-loader": "^0.7.0",
        "slug": "^0.9.1",
        "super-event-emitter": "^4.1.4",
        "ts-loader": "^0.8.2",
        "typings": "^1.4.0",
        "webpack": "^2.0.0-beta",
        "webpack-dev-server": "^1.14.1",
        "write-file-webpack-plugin": "^3.1.8"
