    "scripts": {
        "clear": "rm -rf node_modules/ npm-debug.log app/dist/ typings/",
        "build": "webpack --progress --profile",
        "watch": "webpack --progress -w",
        "dev": "webpack-dev-server --no-info --compress --hot --content-base app",
        "count": "find app/scripts -name '*.js' -not -path 'app/scripts/vendor*' | xargs wc -l | sort -r",
        "postinstall": "typings install env~node --global --save-dev"
    },
    "devDependencies": {
        "assert": "^1.4.1",
        "babel-core": "^6.11.4",
        "babel-loader": "^6.2.4",
        "babel-polyfill": "^6.7.4",
